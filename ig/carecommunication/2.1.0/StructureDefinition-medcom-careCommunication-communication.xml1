      <type>
        <code value="Reference"/>
        <targetProfile value="http://medcomfhir.dk/ig/core/StructureDefinition/medcom-core-encounter"/>
        <aggregation value="bundled"/>
      </type>
      <mustSupport value="true"/>
    </element>
    <element id="Communication.recipient">
      <path value="Communication.recipient"/>
      <short value="Describes a more specific receiver than the MessageHeader.destination.reciever, called a recipient. It may be a careteam a homecare group in the municipality or a named general practitioner."/>
      <type>
        <code value="Reference"/>
        <targetProfile value="http://medcomfhir.dk/ig/core/StructureDefinition/medcom-core-practitionerrole"/>
        <targetProfile value="http://medcomfhir.dk/ig/core/StructureDefinition/medcom-core-careteam"/>
        <aggregation value="bundled"/>
      </type>
      <mustSupport value="true"/>
    </element>
    <element id="Communication.payload">
