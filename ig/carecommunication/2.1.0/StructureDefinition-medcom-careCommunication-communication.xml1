        <expression value="where(category.coding.code != 'regarding-referral').priority.empty()"/>
        <source value="http://medcomfhir.dk/ig/carecommunication/StructureDefinition/medcom-careCommunication-communication"/>
      </constraint>
      <constraint>
        <key value="medcom-careCommunication-6"/>
        <severity value="error"/>
        <human value="There shall exist a Communication.topic when Communication.category = 'other'"/>
        <expression value="iif(category.coding.code != 'other', true, category.coding.code = 'other' and topic.exists())"/>
        <source value="http://medcomfhir.dk/ig/carecommunication/StructureDefinition/medcom-careCommunication-communication"/>
      </constraint>
      <constraint>
        <key value="medcom-careCommunication-7"/>
        <severity value="error"/>
        <human value="There shall exist a practitioner role when using a PractitionerRole as author in a message segment."/>
        <expression value="payload.where(extension('http://medcomfhir.dk/ig/carecommunication/StructureDefinition/medcom-carecommunication-author-extension').exists()).extension.value.reference.resolve().code.coding.code.exists()"/>
        <source value="http://medcomfhir.dk/ig/carecommunication/StructureDefinition/medcom-careCommunication-communication"/>
      </constraint>
      <constraint>
        <key value="medcom-careCommunication-8"/>
        <severity value="error"/>
        <human value="There shall exist a practitioner name when using a Practitioner as author in a message segment."/>
        <expression value="payload.where(extension('http://medcomfhir.dk/ig/carecommunication/StructureDefinition/medcom-carecommunication-author-extension').exists()).extension.value.reference.resolve().practitioner.resolve().name.exists()"/>
        <source value="http://medcomfhir.dk/ig/carecommunication/StructureDefinition/medcom-careCommunication-communication"/>
      </constraint>
      <constraint>
        <key value="medcom-careCommunication-10"/>
        <severity value="error"/>
        <human value="The status shall be 'unknown' or 'entered-in-error'."/>
        <expression value="status='unknown' or status='entered-in-error'"/>
        <source value="http://medcomfhir.dk/ig/carecommunication/StructureDefinition/medcom-careCommunication-communication"/>
      </constraint>
    </element>
    <element id="Communication.extension">
      <path value="Communication.extension"/>
      <slicing>
        <discriminator>
          <type value="value"/>
          <path value="url"/>
        </discriminator>
        <ordered value="false"/>
        <rules value="open"/>
      </slicing>
    </element>
    <element id="Communication.extension:sender">
      <path value="Communication.extension"/>
      <sliceName value="sender"/>
      <min value="0"/>
      <max value="1"/>
