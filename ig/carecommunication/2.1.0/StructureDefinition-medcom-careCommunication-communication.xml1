      <min value="0"/>
      <max value="0"/>
      <type>
        <code value="Extension"/>
        <profile value="http://medcomfhir.dk/ig/carecommunication/StructureDefinition/medcom-carecommunication-attachment-identifier-extension"/>
      </type>
