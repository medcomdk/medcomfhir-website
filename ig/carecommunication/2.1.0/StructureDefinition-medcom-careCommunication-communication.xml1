      </binding>
    </element>
    <element id="Communication.priority">
      <path value="Communication.priority"/>
      <short value="Shall be present if the message priority is known to be ASAP, but is only allowed when the category is 'regarding referral', see medcom-careCommunication-5"/>
      <mustSupport value="true"/>
      <binding>
        <strength value="required"/>
        <valueSet value="http://medcomfhir.dk/ig/terminology/ValueSet/medcom-careCommunication-requestPriority"/>
      </binding>
    </element>
    <element id="Communication.subject">
